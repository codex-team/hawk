{% extends '../layout.twig' %}

{% block body %}
    <h1>{{title}}</h1>
    <p>Welcome to {{title}}</p>

    {% if error is defined %}
        <div>
            Error: {{ error }}
        </div>
    {% else %}
        <div>
            Your token: {{ token }}
        </div>
    {% endif %}

{% endblock %}
