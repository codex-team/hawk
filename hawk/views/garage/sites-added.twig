<div class="garage-sites-added">
    {% if domains.length != 0 %}
        <h1 class="garage-sites-added__title">Select project to view events list</h1>
        {% for domain in domains %}
            {% include 'garage/events/site-item.twig' with { domain: domain } %}
        {% endfor %}
    {% else %}
        {% include 'garage/no-domains.twig' %}
    {% endif %}
</div>