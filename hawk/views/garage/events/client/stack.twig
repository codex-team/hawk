<div class="stack-table">
    {% for item in event.stack %}
        <div class="stack-table__row">
            <div class="stack-table__file">{{ item.func }}@{{ item.file }}</div>
            <div class="stack-table__location">line {{ item.line|number_format }}, col {{ item.col|number_format }}</div>
        </div>
    {% endfor %}
</div>