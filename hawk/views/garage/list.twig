<div class="garage-list">

    {% if events.length != 0 %}

        <div class="garage-list__header">
            Total {{ events.length }} events ordered by date
        </div>

        {% for event in events %}
            {% set tmpl = "garage/events/" ~ event.type ~ ".twig" %}
            {% include tmpl with { domain: domain, event: event} %}
        {% endfor %}

    {% else %}
        You have no errors logged. Looks strange ğŸ˜‘
    {% endif %}
</div>
